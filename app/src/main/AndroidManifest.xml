<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/baseline_task_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/baseline_task_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.DayNight.DarkActionBar"
        tools:targetApi="31">
        <activity
            android:name=".FillSurveyActivity"
            android:exported="false" />
        <activity
            android:name=".SurveyFormActivity"
            android:exported="false" />
        <activity
            android:name=".onBoarding.GettingStarted"
            android:exported="false"
            android:theme="@style/Theme.RemoveSplashScreenTheme" />
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:theme="@style/Theme.RemoveSplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".userRegistration.RegistrationPage"
            android:exported="false"
            android:parentActivityName=".userRegistration.Login" />
        <activity
            android:name=".userRegistration.Login"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>